{
  "cleanUrls": true,
  "trailingSlash": false,
  "i18n": {
    "locales": ["de", "en"],
    "defaultLocale": "de"
  },
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
